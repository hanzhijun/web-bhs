/** loading **/
.base-box {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 101
}
.base-box .gray-back {
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: .7;
  position: absolute;
  left: 0;
  top: 0
}
.base-box .box-confirm {
  width: 6rem;
  overflow: hidden;
  border-radius: .2rem;
  position: absolute;
  left: .75rem;
  top: calc(50vh - 2rem);
}
.base-box .box-confirm .txt {
  padding: .6rem .2rem;
  font-size: .32rem;
  line-height: .44rem;
  text-align: center;
  background-color: #fff;
}
.base-box .box-confirm .btn {
  height: 1.3rem;
  border-top: .5px solid #eee;
}
.base-box .box-confirm .btn a {
   display: inline-block;
  width: 3rem;
  height: 1.3rem;
  line-height: 1.3rem;
  text-align: center;
  font-size: .34rem;
  float: left;
  background-color: #fff;
}
.base-box .box-confirm .btn a.l:active {
  background-color: #f5f5f5;
}
.base-box .box-confirm .btn a.r {
  background-color: #e62343;
  color: #fff;
}
.loading-box .gray-back {
  background-color: #fff;
  opacity: 0
}
.loading-box .box-content {
  width: 120px;
  height: 120px;
  background: rgba(0, 0, 0, 0.5);
  position: absolute;
  left: calc(50vw - 60px);
  top: calc(50vh - 60px);
  border-radius: 5px
}
.loading-box .box-content .img {
  text-align: center;
  padding: 30px 0 20px
}
.loading-box .box-content .img img {
  width: 30px;
  height: 30px
}
.loading-box .box-content .txt {
  text-align: center;
  font-size: 12px;
  color: #f0f0f0
}
.show-toast {
  width: 100%;
  height: 20px;
  text-align: center;
  position: fixed;
  left: 0;
  bottom: 150px;
  z-index: 1001;
  overflow: visible
}
.show-toast span {
  display: inline-block;
  padding: 9px 15px;
  background-color: #333;
  color: #fff;
  font-size: 14px;
  margin: 0 auto;
  border-radius: 5px;
  max-width: 60vw
}
.login-box .box-content {
  width: 200px;
  height: 150px;
  background-color: #fff;
  position: absolute;
  left: calc(50vw - 100px);
  top: calc(50vh - 75px);
  border-radius: 5px;
  text-align: center
}
.login-box .box-content .ts-txt {
  font-size: 16px;
  padding: 20px 0 50px
}
.login-box .box-content .btn button {
  width: 150px;
  line-height: 35px;
  background-color: #0099e9;
  color: #fff;
  font-size: 13px;
  border-radius: 5px
}
.line-base {
  display: block;
  width: 100%;
  height: 5px;
  overflow: hidden;
  border-top: .5px solid #e5e5e5;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1;
  background: none
}
