}
/*文本一行，超出点点点*/
.text-overflow {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap
}
/*文本两行，超出点点点*/
.text-overflow-2 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2
}
.fl {
  float: left
}
.fr {
  float: right
}
.box-sizing {
  box-sizing: border-box
}
.checkbox {
  display:flex;
  align-items:center
}
