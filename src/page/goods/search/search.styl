.out-side-search {
  background-color: #f4f4f4;
  min-height: 100vh;
}

.out-side-search .head-btn-return-txt {
  display: inline-block;
  padding-right: .2rem;
  font-size: .32rem;
  color: #fff;
  float: right;
  line-height: .88rem;
}

.out-side-search .del-all {
  height: 2rem;
  background-color: #fff;
  border-bottom: .5px solid #d9d9d9;
}

.out-side-search .del-all span {
  display: inline-block;
  font-size: .32rem;
  line-height: .32rem;
  color: #262626;
  padding: 1.28rem 0 0 .2rem;
}

.out-side-search .del-all a {
  display: inline-block;
  width: 1rem;
  height: .6rem;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu4.png') no-repeat .18rem .1rem / .5rem 10rem;
  float: right;
  margin-top: 1.12rem;
}

.out-side-search .search-history-list {
  overflow: hidden;
  background-color: #fff;
  padding: 0 .2rem .38rem;
  margin-top: .2rem;
}

.out-side-search .search-history-list li {
  display: block;
  height: 1.1rem;
  position: relative;
  border-bottom: .5px solid #d9d9d9;
}

.out-side-search .search-history-list li span {
  display: block;
  line-height: 1.1rem;
  font-size: .24rem;
  color: #808080;
}

.out-side-search .search-history-list li a {
  width: .8rem;
  height: .5rem;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu4.png') no-repeat .3rem -.4rem / .5rem 10rem;
  position: absolute;
  top: .4rem;
  right: 0;
}