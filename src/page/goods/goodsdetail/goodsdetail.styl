.out-side-goodsdetail {
  width: 100vw;
  min-height: 100vh;
  padding: .88rem 0 1.2rem;
  overflow: hidden;
}

.out-side-goodsdetail .banner {
  height: 100vw;
}

.out-side-goodsdetail .banner .slide-image {
  width: 100vw;
  height: 100vw
}

.out-side-goodsdetail .goods-main {
  overflow: hidden;
  background-color: #fff;
  margin-bottom: .25rem;
}

.out-side-goodsdetail .detai-info1 {
  height: .85rem;
  padding: 0 .2rem;
  overflow: hidden;
}

.out-side-goodsdetail .detai-info1 .icon-rmb {
  display: inline-block;
  line-height: .4rem;
  float: left;
  margin: .44rem 0 0 0;
}

.out-side-goodsdetail .detai-info1 .price-now {
  display: inline-block;
  font-size: .64rem;
  line-height: .64rem;
  float: left;
  margin: .24rem .05rem 0 0;
}

.out-side-goodsdetail .detai-info1 .price-old {
  display: inline-block;
  line-height: .4rem;
  float: left;
  margin: .44rem 0 0 .08rem;
}

.out-side-goodsdetail .detai-info1 .goods-collect {
  display: inline-block;
  width: .6rem;
  float: right;
  text-align: center;
  margin: .25rem 0 0 0;
}

.out-side-goodsdetail .detai-info1 .goods-collect i {
  display: block;
  height: .33rem;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu4.png') no-repeat .13rem -2.5rem/0.5rem 10rem;
}

.out-side-goodsdetail .detai-info1 .goods-collect.this-over i {
  background: url('https://api.duishangbao.cn/upload/image/icon_zu4.png') no-repeat .13rem -3rem/0.5rem 10rem;
}

.out-side-goodsdetail .detai-info1 .goods-num {
  display: inline-block;
  line-height: .4rem;
  float: right;
  margin: .44rem .08rem 0 0;
}

.out-side-goodsdetail .detai-info2 {
  width: 5.5rem;
  height: .8rem;
  margin: .02rem 1.4rem .02rem .2rem;
  background: url('https://api.duishangbao.cn/upload/image/qipao_jiao4.png') repeat-x 0 0/1.5rem .8rem;
  overflow: hidden;
}

.out-side-goodsdetail .detai-info2-bj2 {
  width: 5.5rem;
  height: .8rem;
  background: url('https://api.duishangbao.cn/upload/image/qipao_jiao3.png') no-repeat 0 0/1.5rem .8rem;
}

.out-side-goodsdetail .detai-info2-bj3 {
  width: 5.5rem;
  height: .8rem;
  line-height: .54rem;
  background: url('https://api.duishangbao.cn/upload/image/qipao_jiao5.png') no-repeat right/1.5rem .8rem;
  display: flex;
  justify-content: space-around;
  text-align: center;
  position: relative;
  padding-top: .27rem
}

.out-side-goodsdetail .detai-info2.help-pay,
.out-side-goodsdetail .help-pay .detai-info2-bj2,
.out-side-goodsdetail .help-pay .detai-info2-bj3 {
  width: 7.1rem;
}

.out-side-goodsdetail .detai-info2-bj3 .line-ge {
  display: inline-block;
  width: 1px;
  height: .54rem;
  background: url('https://api.duishangbao.cn/upload/image/qipao_jiao6.png') no-repeat right/1px 54px;
}

.out-side-goodsdetail .detai-info2-bj3 span {
  line-height: .58rem;
}

.out-side-goodsdetail .goods-name {
  line-height: .52rem;
  font-size: .32rem;
  color: #000;
  padding: .22rem .2rem .1rem;
}

.out-side-goodsdetail .goods-name i {
  display: inline-block;
  height: .46rem;
  line-height: .46rem;
  background: #fc6c15 url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat .1rem -4.95rem / .7rem 10rem;
  padding: 0 .1rem 0 .55rem;
  font-size: .32rem;
  color: #fff;
  border-radius: .05rem;
  margin-right: .1rem;
}

.out-side-goodsdetail .goods-main .border-line-heng-e6e6e6 {
  width: 7.1rem;
  margin: 0 auto;
}

.out-side-goodsdetail .goods-mess-bar {
  width: 7.1rem;
  height: .78rem;
  line-height: .78rem;
  margin: 0 auto;
}

.out-side-goodsdetail .goods-mess-bar .title {
  display: inline-block;
  width: .9rem;
  float: left;
}

.out-side-goodsdetail .goods-mess-bar .this-value {
  display: inline-block;
  width: 6rem;
  float: left;
}

.out-side-goodsdetail .goods-mess-bar .icon {
  display: inline-block;
  width: .16rem;
  height: .28rem;
  float: right;
  margin-top: .26rem;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu4.png') no-repeat 0 -4rem/.5rem 10rem;
}

.out-side-goodsdetail .goods-explain {
  height: .65rem;
  line-height: .65rem;
  padding: 0 .2rem;
  background-color: #ffeaed;
}

.out-side-goodsdetail .goods-explain span {
  display: inline-block;
  min-width: .9rem;
  color: #cc250e;
  float: left;
}

.out-side-goodsdetail .goods-detail {
  overflow: hidden;
  background-color: #fff;
}

.out-side-goodsdetail .goods-detail .detail-title {
  text-align: center;
  padding: .4rem 0 .2rem;
}

.out-side-goodsdetail .goods-detail .detail-title .txt {
  display: block;
  text-align: center;
  line-height: .5rem;
}

.out-side-goodsdetail .goods-detail .detail-title i {
  clear: both;
  display: inline-block;
  width: .18rem;
  height: .26rem;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu4.png') no-repeat 0 -5rem/.5rem 10rem;
  margin: 0 auto;
}

.out-side-goodsdetail .detail-img img {
  width: 100vw;
}

.out-side-goodsdetail .detail-footer {
  width: 100vw;
  height: 1rem;
  background-color: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  text-align: center;
  opacity: .99;
}

.out-side-goodsdetail .detail-footer .border-line-heng-e6e6e6 {
  width: 100%;
  height: 1px;
  float: left;
}

.out-side-goodsdetail .detail-footer .help-pay-btn {
  display: block;
  width: 5.5rem;
  background-color: #e62343;
  color: #f4f4f4;
  padding: .16rem 0;
  margin: .06rem auto;
  border-radius: .9rem;
}

.out-side-goodsdetail .detail-footer .to-order {
  width: 2.88rem;
  height: 1rem;
  line-height: 1rem;
  background-color: #e62343;
  text-align: center;
  position: absolute;
  right: 0;
  top: 0;
}

.out-side-goodsdetail .detail-footer .to-cart {
  width: 1.75rem;
  padding-top: .1rem;
  float: left;
  text-align: center;
  position: relative;
}

.out-side-goodsdetail .detail-footer .to-cart .icon {
  display: block;
  height: .5rem;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu3.png') no-repeat .58rem -2.8rem/1rem 7rem;
}

.out-side-goodsdetail .detail-footer .to-cart .num {
  display: inline-block;
  position: absolute;
  font-size: .18rem;
  color: #fff;
  background-color: #e62343;
  width: .38rem;
  line-height: .1rem;
  padding: .14rem 0;
  left: 1rem;
  top: .03rem;
  border-radius: 50%;
  overflow: hidden;
  opacity: .9;
  z-index: 2;
}

.out-side-goodsdetail .detail-footer .border-line-shu-e6e6e6 {
  height: .56rem;
  float: left;
  margin-top: .22rem;
}

.out-side-goodsdetail .detail-footer .in-cart {
  width: 2.68rem;
  height: 1rem;
  line-height: 1rem;
  text-align: center;
  float: left;
  margin-left: .1rem;
}

.out-side-goodsdetail .box-select-sku {
  width: 100vw;
  height: 100vh;
  display: none;
}

.out-side-goodsdetail .box-select-sku.this-show {
  display: block;
}

.out-side-goodsdetail .box-select-sku .box-sku-main {
  width: 100vw;
  height: 8.79rem;
  background-color: #fff;
  position: absolute;
  left: 0;
  bottom: 0;
}

.out-side-goodsdetail .box-select-sku header {
  display: block;
  height: .95rem;
  line-height: .95rem;
  padding-left: .2rem;
  overflow: hidden;
  background-color: #fff;
}

.out-side-goodsdetail .box-select-sku header a {
  display: inline-block;
  width: .5rem;
  height: .5rem;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu4.png') no-repeat 0 -3.5rem/.5rem 10rem;
  float: right;
  margin: .3rem .1rem 0 0;
}

.out-side-goodsdetail .box-select-sku .info-bar {
  height: 2.6rem;
}

.out-side-goodsdetail .box-select-sku .info-img {
  width: 2rem;
  height: 2rem;
  overflow: hidden;
  float: left;
  margin: .3rem .3rem 0 .2rem;
}

.out-side-goodsdetail .box-select-sku .info-img img {
  width: 2rem;
  height: 2rem;
  border-radius: .1rem;
}

.out-side-goodsdetail .info-mess {
  width: 4.8rem;
  height: 2.1rem;
  float: left;
  margin-top: .3rem;
}

.out-side-goodsdetail .info-mess .price {
  padding: .1rem 0 .1rem;
}

.out-side-goodsdetail .info-mess .price-sku {
  font-size: .5rem;
}

.out-side-goodsdetail .info-mess .note-txt {
  padding-top: .15rem;
  line-height: .36rem;
}

.out-side-goodsdetail .info-select {
  height: 4.23rem;
  overflow-y: auto;
}

.out-side-goodsdetail .info-select ul {
  display: block;
  padding: .16rem;
}

.out-side-goodsdetail .info-select .type-name {
  display: block;
  line-height: .5rem;
}

.out-side-goodsdetail .info-select .type-box {
  display: block;
  padding-top: .18rem;
  overflow: hidden;
}

.out-side-goodsdetail .info-select .type-box span {
  display: inline-block;
  padding: .1rem .38rem;
  border: .5px solid #ccc;
  color: #1a1a1a;
  float: left;
  margin: 0 .2rem .3rem 0;
  border-radius: .6rem
}

.out-side-goodsdetail .info-select .type-box span.this-over {
  border: .5px solid #e62343;
  color: #e62343;
  background-color: #fff7f8;
}

.out-side-goodsdetail .info-select .btn-box {
  overflow: hidden;
  padding: .15rem 0 .25rem;
}

.out-side-goodsdetail .info-select .btn-box span {
  display: inline-block;
  font-size: .24rem;
  color: #1a1a1a;
  padding: .09rem .2rem;
  float: left;
  border-top: .8px solid #ddd;
  border-bottom: .8px solid #ddd;
  border-right: .8px solid #ddd;
}

.out-side-goodsdetail .info-select .btn-box span.l {
  border-left: .8px solid #ddd;
  border-top-left-radius: 2px;
  border-bottom-left-radius: 2px;
}

.out-side-goodsdetail .info-select .btn-box span.r {
  border-top-right-radius: 2px;
  border-bottom-right-radius: 2px;
}

.out-side-goodsdetail .info-select .btn-box span.l:active,
.out-side-goodsdetail .info-select .btn-box span.r:active {
  background-color: #f0f0f0;
}

.out-side-goodsdetail .box-select-sku footer {
  display: block;
  height: .98rem;
}

.out-side-goodsdetail .box-select-sku footer a {
  display: block;
  width: 50%;
  height: .98rem;
  text-align: center;
  line-height: .98rem;
  float: left;
  background-color: #fc6c15;
}

.out-side-goodsdetail .box-select-sku footer a.go-pay {
  background-color: #e62343;
}